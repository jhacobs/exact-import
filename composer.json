{
    "name": "jacobaretz/exact-import",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Aretz",
            "email": "jacob.aretz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/database": "^6.0|^7.0",
        "illuminate/bus": "^6.0|^7.0",
        "illuminate/queue": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0",
        "illuminate/console": "^6.0|^7.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.2",
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Exact\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Exact\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Exact\\ExactServiceProvider"
            ],
            "aliases": {
                "Exact": "Exact\\Facades\\Exact"
            }
        }
    }
}
